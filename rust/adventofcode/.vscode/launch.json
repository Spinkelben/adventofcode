    "version": "0.2.0",
    "configurations": [
        {
            "type": "lldb",
            "request": "launch",
            "name": "Launch",
            "cargo": {
                "args": [
                    "build",
                    "--bin=adventofcode",
                    "--package=adventofcode",
                ],
                "filter": {
                    "name": "adventofcode",
                    "kind": "bin"
                }
            },
            "args": ["--year", "2022", "--day" , "5"],
            "cwd": "${workspaceFolder}",

        }

    ]
}